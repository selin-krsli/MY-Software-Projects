@{
    ViewBag.Title = "CreateEducation";
    Layout = "~/Views/Admin/Index.cshtml";
}
<div class="alert alert-success"><strong>Yeni Eğitim Bilgisi Girişi</strong></div>
<form method="post">
    <label>Başlık</label>
    <input type="text" class="form-control" name="Title" />
    <br />
    <label>Alt Başlık</label>
    <input type="text" class="form-control" name="SubTitle" />
    <br />
    <label>Bulunulan Tarih Aralığı</label>
    <input type="text" class="form-control" name="DateRange" />
    <br />
    <label>Açıklama</label>
    <textarea rows="4" class="form-control" name="Description"></textarea>
    <br />
    <button class="btn btn-outline-info">Eğitim Bilgisini Kaydet</button>
</form>

