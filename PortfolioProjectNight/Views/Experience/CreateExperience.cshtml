@model PortfolioProjectNight.Models.Experience
@{
    ViewBag.Title = "CreateExperience";
    Layout = "~/Views/Admin/Index.cshtml";
}
<div class="alert alert-success"><strong>Yeni Deneyim Ekleme Sayfası</strong></div>
<form method="post">
    <label>Deneyim Başlığı</label>
    @Html.TextBoxFor(x=>x.Title, new {@class="form-control"})
    <br />
    <label>Deneyim Alt Başlığı</label>
    @Html.TextBoxFor(x=>x.SubTitle, new {@class="form-control"})
    <br />
    <label>Deneyim Açıklaması</label>
    @Html.TextAreaFor(x=>x.Description, new {@class="form-control"})
    <br />
    <button class="btn btn-outline-info">Deneyim Oluştur</button>
</form>

